{
  "name": "understreck",
  "version": "2.0.0",
  "description": "Recreation of some of lodash's modules. Don't use this, use lodash!",
  "main": "index.js",
  "keywords": [
    "utility",
    "underscore",
    "lodash"
  ],
  "author": "Emil Johansson <emiljohansson@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack understreck.js dist/understreck.js && babel dist/understreck.js >> dist/understreck.js",
    "minify": "uglifyjs dist/understreck.js >> dist/understreck.min.js",
    "benchmark": "node benchmarks",
    "lint": "standard {benchmarks,src}/**/* || exit 0",
    "test": "ava src/**/test.js"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "benchmark": "^2.1.0",
    "chalk": "^2.0.1",
    "list-files": "^1.1.4",
    "sinon": "^2.3.8",
    "standard": "^10.0.2",
    "uglify-es": "^3.0.24",
    "webpack": "^3.3.0"
  },
  "dependencies": {
    "array-insertat": "^2.0.0"
  }
}
